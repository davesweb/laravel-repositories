{
    "name": "davesweb/repositories",
    "description": "Easy repository package for Laravel 5",
    "type": "library",
    "require": {
        "php": "^7.1",
        "laravel/framework": "^5.5",
        "illuminate/support": "^5.5",
        "illuminate/database": "^5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "davesweb/mockery-helper": "^1.1",
        "mockery/mockery": "^1.0",
        "friendsofphp/php-cs-fixer": "^2.10",
        "phpro/grumphp": "^0.13.1"
    },
    "autoload": {
        "psr-4": {
            "Davesweb\\Repositories\\": "src/",
            "Davesweb\\Repositories\\Tests\\": "tests/"
        }
    },
    "autoload-dev": {
        "psr-4": { "Davesweb\\Repositories\\Tests\\": "tests/" }
    },
    "license": "MIT",
    "extra": {
        "laravel": {
            "providers": [
                "Davesweb\\Repositories\\RepositoryServiceProvider"
            ]
        }
    }
}
